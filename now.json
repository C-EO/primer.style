{
  "version": 2,
  "name": "primer-style",
  "alias": "primer-style.now.sh",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {"distDir": "public"}
    }
  ],
  "routes": [
    {"src": "/components**", "dest": "primer-components.now.sh"},
    {"src": "/css**", "dest": "primer-css.now.sh"},
    {"src": "/design**", "dest": "primer-design.now.sh"},
    {"src": "/blueprints**", "dest": "primer-blueprints.now.sh"},
    {"src": "/presentations**", "dest": "primer-presentations.now.sh"}
  ]
}
